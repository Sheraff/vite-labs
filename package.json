{
  "name": "labs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "echo \"node:$(node -v) pnpm:v$(pnpm -v)\\n\" && vite",
    "build": "vite build",
    "lint": "oxlint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "types": "tsgo",
    "format": "oxfmt",
    "format:check": "oxfmt --check"
  },
  "imports": {
    "#router": "./src/router.ts",
    "#components/*": "./src/components/*.tsx",
    "#file-router/*": "./src/file-router/*.tsx",
    "#*": {
      "types": [
        "./src/pages/*.tsx",
        "./src/pages/*.ts",
        "./src/pages/*.css"
      ],
      "default": "./src/pages/*"
    }
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "magic-string": "^0.30.19",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20251204.1",
    "@vitejs/plugin-react": "5.1.1",
    "@webgpu/types": "0.1.67",
    "globals": "16.4.0",
    "oxc-parser": "^0.101.0",
    "oxfmt": "0.16.0",
    "oxlint": "^1.31.0",
    "vite": "8.0.0-beta.0"
  },
  "packageManager": "pnpm@10.18.0"
}